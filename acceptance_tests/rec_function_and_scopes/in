def main(){
    print(fibonacci(0) + " ");
    print(fibonacci(1) + " ");
    print(fibonacci(2) + " ");
    print(fibonacci_double(3) + " ");
    print(fibonacci_double(4) + " ");
    print(fibonacci_double(5) + " ");
    print(fibonacci_double(6) + " ");
    print(fibonacci(7) + " ");
    print(fibonacci(8) + " ");
    print(fibonacci(9) + " ");

    var a: int = 2;
    print(a);
    {
        print(a);
        var a: int = 4;
        a += 5;
        print(a);
    }
    print(a);
}

def fibonacci(n: int): int {
    if(n == 0)
        return 0;
    else if(n == 1)
        return 1;
    else
        return fibonacci(n - 1) + fibonacci(n - 2); 
}

def fibonacci_double(n: int): double {
    if(n == 0)
        return 0.0;
    else if(n == 1)
        return 1.0;
    else
        return fibonacci_double(n - 1) + fibonacci_double(n - 2); 
}